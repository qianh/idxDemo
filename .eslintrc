{
  "extends": ["eslint-config-umi", "tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "no-unused-labels": "error",
    "no-unused-vars": "error",
    "no-unused-expressions": 1,
    "react/no-unescaped-entities": "error",
    "no-use-before-define": "error"
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
